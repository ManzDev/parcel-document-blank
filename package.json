{
  "name": "parcel-template-blank",
  "version": "2.0.0",
  "description": "Blank template project starter-kit with Parcel v2",
  "scripts": {
    "dev": "parcel serve src/index.html",
    "watch": "parcel watch src/index.html",
    "report": "PARCEL_BUNDLE_ANALYZER=1 BUNDLE_BUDDY=1 parcel build src/index.html --dist-dir build && live-server parcel-bundle-reports/default.html",
    "build": "parcel build src/index.html --dist-dir build --public-url /",
    "gh-pages": "parcel build src/index.html --dist-dir build --public-url /parcel-template-blank/",
    "deploy": "gh-pages -d build",
    "lintjs": "eslint src/js/**/*.js",
    "lintcss": "stylelint src/css/**/*.css",
    "clean:cache": "rm -rf .parcel-cache dist build",
    "clean:all": "rm -rf node_modules package-lock.json .parcel-cache dist build"
  },
  "keywords": [
    "parcel",
    "template",
    "html5",
    "css3",
    "postcss",
    "stylelint",
    "javascript",
    "eslint"
  ],
  "author": "Manz",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "gh-pages": "^3.1.0",
    "htmlhint": "^0.14.1",
    "live-server": "^1.2.1",
    "parcel": "^2.0.0-nightly.425",
    "postcss-font-magician": "^3.0.0",
    "postcss-mixins": "^7.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0"
  }
}
