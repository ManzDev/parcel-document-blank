{
  "name": "parcel-template-blank",
  "version": "1.0.3",
  "description": "Blank template project starter-kit with Parcel Bundler",
  "main": "src/index.html",
  "scripts": {
    "dev": "parcel serve src/index.html --global global --open",
    "watch": "parcel watch src/index.html --global global",
    "build": "rm -rf build/* && parcel build src/index.html -d build --global global --public-url /",
    "clean:cache": "rm -rf .cache dist build",
    "clean:all": "rm -rf node_modules package-lock.json .cache dist build",
    "deploy": "git subtree push --prefix build origin gh-pages"
  },
  "keywords": [
    "parcel",
    "template",
    "html5",
    "css3",
    "postcss",
    "stylelint",
    "javascript",
    "eslint"
  ],
  "author": "Manz",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "htmlhint": "^0.11.0",
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "postcss-clean": "^1.1.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-font-magician": "^2.2.2",
    "postcss-mixins": "^6.2.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.0",
    "stylelint": "^11.0.0",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-standard": "^19.0.0"
  },
  "dependencies": {}
}
